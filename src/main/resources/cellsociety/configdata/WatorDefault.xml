<simulation>
  <type>WATOR</type>
  <title>Wa-Tor Equilibrium Test</title>
  <author>Jacob You</author>
  <description>A larger Wa-Tor simulation designed to reach a stable equilibrium of fish and sharks.</description>
  <parameters>
    <baseSharkEnergy>5</baseSharkEnergy>
    <fishEnergyGain>3</fishEnergyGain>
    <sharkReproductionTime>20</sharkReproductionTime>
    <fishReproductionTime>1</fishReproductionTime>
  </parameters>
  <width>15</width>
  <height>15</height>
  <defaultSpeed>1</defaultSpeed>
  <initialStates>
    <row>0 0 1 0 2 2 0 1 0 1 0 0 1 2 1</row>
    <row>1 0 0 1 0 1 0 0 0 1 2 2 0 1 2</row>
    <row>0 1 0 1 1 0 2 0 1 0 0 1 2 0 1</row>
    <row>2 0 1 0 1 1 1 2 0 1 1 0 0 2 0</row>
    <row>0 0 1 2 0 1 0 2 1 0 2 1 1 0 2</row>
    <row>1 0 1 0 1 0 1 0 0 2 1 0 0 1 0</row>
    <row>2 1 0 2 0 1 0 1 2 0 1 1 2 0 0</row>
    <row>0 0 2 1 0 1 1 0 1 1 0 1 2 1 0</row>
    <row>1 2 0 0 1 1 0 1 0 1 1 0 1 2 0</row>
    <row>1 0 2 1 0 0 1 1 0 0 1 1 2 0 1</row>
    <row>0 1 0 1 1 1 1 1 0 2 0 1 0 1 2</row>
    <row>2 0 1 0 2 0 0 1 1 1 0 0 1 0 1</row>
    <row>1 2 1 2 0 1 0 0 1 0 1 0 1 2 1</row>
    <row>0 0 2 1 0 1 1 1 0 1 2 0 0 0 2</row>
    <row>1 0 1 0 2 1 2 0 0 1 0 2 1 0 2</row>
  </initialStates>
</simulation>
