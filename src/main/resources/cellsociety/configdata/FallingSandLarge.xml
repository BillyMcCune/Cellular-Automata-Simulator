      <cell state="2"/>
       
      <cell state="2"/>
       
      <cell state="0"/>
       
      <cell state="0"/>
       
    </row>
    

    
    
    <row>
       
      <cell state="1"/>
       
      <cell state="0"/>
       
      <cell state="2"/>
       
      <cell state="0"/>
       
      <cell state="1"/>
      
      
      <cell state="1"/>
       
