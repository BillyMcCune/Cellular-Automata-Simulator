<?xml version="1.0" ?>
<simulation>
  <type>SUGAR</type>
  <title>Basic SugarScape Simulation</title>
  <author>Jacob You</author>
  <description>
    Demonstrates agents moving to the patch with the most sugar within a given vision range,
    consuming sugar, and regrowing sugar at patches every specified interval.
  </description>

  <parameters>
    <doubleParameter name="vision">2</doubleParameter>
    <doubleParameter name="sugarMetabolism">1</doubleParameter>
    <doubleParameter name="sugarGrowBackRate">2</doubleParameter>
    <doubleParameter name="sugarGrowBackInterval">3</doubleParameter>
  </parameters>

  <cellShapeType>Square</cellShapeType>
  <gridEdgeType>BASE</gridEdgeType>
  <neighborArrangementType>Custom</neighborArrangementType>

  <width>5</width>
  <height>5</height>
  <defaultSpeed>10</defaultSpeed>

  <initialCells>
    <!-- Row 0 -->
    <row>
      <cell state="1" sugarAmount="15" maxSugar="15" agentSugar="10"/>
      <cell state="0" sugarAmount="3" maxSugar="10"/>
      <cell state="0" sugarAmount="10" maxSugar="10"/>
      <cell state="1" sugarAmount="5" maxSugar="10" agentSugar="5"/>
      <cell state="0" sugarAmount="0" maxSugar="8"/>
    </row>
    <!-- Row 1 -->
    <row>
      <cell state="0" sugarAmount="6" maxSugar="8"/>
      <cell state="1" sugarAmount="2" maxSugar="8" agentSugar="12"/>
      <cell state="0" sugarAmount="7" maxSugar="10"/>
      <cell state="0" sugarAmount="8" maxSugar="8"/>
      <cell state="1" sugarAmount="4" maxSugar="10" agentSugar="6"/>
    </row>
    <!-- Row 2 -->
    <row>
      <cell state="0" sugarAmount="9" maxSugar="9"/>
      <cell state="0" sugarAmount="4" maxSugar="10"/>
      <cell state="0" sugarAmount="5" maxSugar="5"/>
      <cell state="1" sugarAmount="2" maxSugar="8" agentSugar="8"/>
      <cell state="0" sugarAmount="6" maxSugar="10"/>
    </row>
    <!-- Row 3 -->
    <row>
      <cell state="0" sugarAmount="7" maxSugar="10"/>
      <cell state="1" sugarAmount="0" maxSugar="10" agentSugar="10"/>
      <cell state="0" sugarAmount="3" maxSugar="10"/>
      <cell state="0" sugarAmount="3" maxSugar="5"/>
      <cell state="0" sugarAmount="9" maxSugar="9"/>
    </row>
    <!-- Row 4 -->
    <row>
      <cell state="0" sugarAmount="10" maxSugar="10"/>
      <cell state="1" sugarAmount="2" maxSugar="8" agentSugar="7"/>
      <cell state="0" sugarAmount="0" maxSugar="5"/>
      <cell state="0" sugarAmount="5" maxSugar="5"/>
      <cell state="0" sugarAmount="0" maxSugar="8"/>
    </row>
  </initialCells>

  <!-- 0=EMPTY, 1=AGENT -->
  <acceptedStates>0 1</acceptedStates>
</simulation>
